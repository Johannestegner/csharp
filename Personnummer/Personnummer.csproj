<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Company>personnummer</Company>
    <Authors>Johannes Tegnér, Fredrik Forsmo</Authors>
    <Description>Verify Swedish social security numbers.</Description>
    <PackageLicenseUrl>https://github.com/personnummer/csharp/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/personnummer/csharp</PackageProjectUrl>
    <RepositoryUrl>https://github.com/personnummer/csharp</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Validation, Social security numbers, Swedish, Sweden, Verify</PackageTags>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicense>https://github.com/personnummer/csharp/blob/master/LICENSE</PackageLicense>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.2</Version>
    <AssemblyVersion>1.0.2.0</AssemblyVersion>
    <FileVersion>1.0.2.0</FileVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <WarningLevel>4</WarningLevel>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <WarningsAsErrors />
  </PropertyGroup>

</Project>
